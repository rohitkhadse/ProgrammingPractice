package com.collections;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;
import java.util.Map;

public class TestCar {

	public static void main(String[] args) {
		Car c1= new Car("green",2017,"sedan",2000);
		Car c2= new Car("red",2017,"suv",33000);
		Car c3= new Car("green",2016,"truck",40000);
		Car c4= new Car("black",2015,"sedan",23000);
		
		List<Car> al = new ArrayList<Car>();

		Map<Integer,Car> hmap = new HashMap<Integer,Car>();
		for(int i=0;i<10000;i++){
			hmap.put(i,new Car(i+"a",2000+i,i+"sedan",i*100));
			al.add(new Car(i+"a",2000+i,i+"sedan",i*100));
		}
		
		searchMap(hmap,2777);
		searchArrayList(al,2777);
	}

	private static void searchArrayList(List<Car> al, int i) {
		for(Car c : al){
			if(i==c.getYear()){
				System.out.println(c);
			}
		}
		
	}

	private static void searchMap(Map<Integer, Car> hmap, int i) {
		for(Map.Entry<Integer,Car> m:hmap.entrySet()){
			Car c = m.getValue();
			if(i == c.getYear()){
				System.out.println(c);
			}
		}
	}

}
